{
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "all_frames": false,
      "css": ["cnt.css"],
      "js": ["cnt.js"],
      "matches": ["https://mail.google.com/*"],
      "run_at": "document_start"
    }
  ],
  "description": "Better reading experience on Gmail",
  "icons": {
    "19": "img/i19.png",
    "48": "img/i48.png",
    "64": "img/i64.png"
  },
  "manifest_version": 2,
  "name": "V7 Gmail Zoom",
  "permissions": ["storage"],
  "version": "2.1",
  "web_accessible_resources": ["img/*.*"]
}
